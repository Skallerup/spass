 ADD PRIMARY KEY (`meta_id`), ADD KEY `comment_id` (`comment_id`), ADD KEY `meta_key` (`meta_key`(191));

--
-- Indeks for tabel `fl_comments`
--
ALTER TABLE `fl_comments`
 ADD PRIMARY KEY (`comment_ID`), ADD KEY `comment_post_ID` (`comment_post_ID`), ADD KEY `comment_approved_date_gmt` (`comment_approved`,`comment_date_gmt`), ADD KEY `comment_date_gmt` (`comment_date_gmt`), ADD KEY `comment_parent` (`comment_parent`), ADD KEY `comment_author_email` (`comment_author_email`(10));

--
-- Indeks for tabel `fl_cpk_wpcsv_export_queue`
--
ALTER TABLE `fl_cpk_wpcsv_export_queue`
 ADD PRIMARY KEY (`id`);

--
-- Indeks for tabel `fl_cpk_wpcsv_log`
--
ALTER TABLE `fl_cpk_wpcsv_log`
 ADD PRIMARY KEY (`id`);

--
-- Indeks for tabel `fl_itsec_lockouts`
--
ALTER TABLE `fl_itsec_lockouts`
 ADD PRIMARY KEY (`lockout_id`), ADD KEY `lockout_expire_gmt` (`lockout_expire_gmt`), ADD KEY `lockout_host` (`lockout_host`), ADD KEY `lockout_user` (`lockout_user`), ADD KEY `lockout_username` (`lockout_username`), ADD KEY `lockout_active` (`lockout_active`);

--
-- Indeks for tabel `fl_itsec_log`
--
ALTER TABLE `fl_itsec_log`
 ADD PRIMARY KEY (`log_id`), ADD KEY `log_type` (`log_type`), ADD KEY `log_date_gmt` (`log_date_gmt`);

--
-- Indeks for tabel `fl_itsec_temp`
--
ALTER TABLE `fl_itsec_temp`
